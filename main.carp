(load "config.carp")

(load "lib/tonc.carp")

(load "src/interfaces.carp")
(load "src/models.carp")
(load "src/collisions.carp")
(load "src/update.carp")
(load "src/render.carp")

(defn main []
 (do
  (Display.set-control (bit-or Display.mode3 Display.background2))
  (let [world (World.new)]
    (while-do true
     (Key.poll)
     (set! world (update world))
     (Video.vsync)
     (render &world)))))

